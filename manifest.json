{
"manifest_version": 3,
"name": "UTubeSummarizer",
"action": {
    "default_icon": {
        "16": "youtube.png",
        "32": "youtube.png",
        "48": "youtube.png",
        "128": "youtube.png"
    },
    "default_popup": "popup.html"
},
"icons": {
    "16": "youtube.png",
    "32": "youtube.png",
    "48": "youtube.png",
    "128": "youtube.png"
},
"description": "An extension to provide a summarized transcript of a YouTube Subtitle eligible Video.",
"version": "1.0",
"permissions": ["activeTab"],
"host_permissions": ["http://127.0.0.1:5000/*"]
}